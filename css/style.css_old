	min-height: 60px;
}

.footer-widget.event ul li:hover .thumbnail-img img {
	transform: scale(1.2,1.2);
}

.footer-widget.event ul li:hover h5 {
	color: #ddd;
}

.footer-widget.menulinks ul li {
	position: relative;
