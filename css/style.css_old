    left: -1px;
    background: #75c01f;
    padding: 11px 18px;
    border-radius: 30px;
    font-size: 20px;
    color: #fff;
}

.error-inner p.desc {
	font-size: 17px;
}


/*=======================
PAGE SIDEBAR CSS
=======================*/

.sidebar h4 {
	font-size: 20px;
	margin: 0 0 20px;
}

.contactform label {
	font-size: 15px;
}

.contactform .form-control {
	height: 38px;
	border-radius: 0px;
}

.contactform textarea.form-control {
	height: auto;
}

.sub-btn {
    background: #75c01f;
    color: #fff;
    padding: 8px 20px;
    border-radius: 0px;
    transition: all 0.3s ease-in-out;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
}

.sub-btn:hover {
	background: #416b12;
}

/*============= PAGES CSS =============*/

.pageSliderWrapper {
	position: relative;
}

#owl-one .slider-item:before {
	position: absolute;
	content: "";
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	background: #b8ae2b47;
}

#owl-one .slider-item img {
	width: 100%;
}

#owl-one .owl-dots {
    position: absolute;
    bottom: 5%;
    left: 50%;
    z-index: 999;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
}

#owl-one .owl-dots .owl-dot {
    height: 28px;
    width: 28px;
    border: 3px solid #fff;
    border-radius: 100%;
    margin-right: 12px;
}

#owl-one .owl-dots .owl-dot:last-child {
	margin-right: 0px;
}

#owl-one .owl-dots .owl-dot span {
    width: 14px;
    height: 14px;
    margin: 0px 4px;
    background: #fff;
    display: block;
    border-radius: 30px;
    transition: all 0.1s ease-in-out;
    -webkit-transition: all 0.1s ease-in-out;
    -moz-transition: all 0.1s ease-in-out;
    visibility: hidden;
    opacity: 0;
}

#owl-one .owl-dots .owl-dot.active span, 
#owl-one .owl-dots .owl-dot:hover span {
	background: #fff;
	visibility: visible;
	opacity: 1;
}

.slider-contentWrapper {
	position: absolute;
	bottom: 13%;
	left: 0;
	width: 100%;
	text-align: center;
	z-index: 2;
}

.slider-contentWrapper .slider-content {
	color: #fff;
}

.slider-contentWrapper .slider-content p {
	font-family: 'Arial Rounded MT';
}

.slider-contentWrapper .slider-content .thumbnail-img {
	margin: 0 0 15px;
}

.category-contentWrapper {
	position: absolute;
	top: 20px;
	left: 0;
	width: 100%;
	text-align: center;
	z-index: 2;
}

.category-contentWrapper .category-content {
	text-align: left;
}

.category-contentWrapper .category-content ul li {
	margin: 0 0 20px;
