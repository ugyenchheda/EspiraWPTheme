	left: 0;
	font-family: 'FontAwesome';
}

.footer-widget.event ul li h5 {
	font-size: 16px;
	margin: 0 0 5px;
}

.footer-widget.event ul li p {
	font-size: 13px;
	line-height: 19px;
}


.footer-widget .social-icons li {
	display: inline-block;
	vertical-align: top;
	margin-right: 10px;
}

.footer-widget .social-icons li:last-child {
	margin-right: 0px;
}

.footer-widget .social-icons li a {
	display: block;
	font-size: 20px;
}



/*============================
ARCHIVE PAGE CSS
=============================*/

.pageinnerbanner {
	background: url('../images/pagebanner-img.jpg')no-repeat;
	background-attachment: fixed; 
	background-position: center;
	background-size: cover;
	position: relative;
}

.banner-inner {
	height: 400px;
	display: flex;
	align-items: center;
	justify-content: center;
}

.pageinnerbanner:before {
	content: "";
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.5);
}

.pageinnerbanner h3 {
	color: #fff;
	font-size: 30px;
	font-family: inherit;
	font-weight: bold;
	text-align: center;
}

.pagecontent.category {
	padding: 60px 0;
}

.cat-heading {
	margin: 0 0 30px;
}

.cat-heading h4 {
	font-size: 25px;
    font-family: inherit;
    color: #75c01f;
}

.articleWrapper {
	background: #f9f9f9;
    padding: 20px 20px;
}

.article-content h5 {
	color: #383131;
	margin: 0 0 8px;
	font-family: inherit;
	font-size: 18px;
	transition: all 0.2s ease-in-out;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
}

.posted-time {
	display: block;
	font-size: 14px;
	margin: 0 0 15px;
	color: #7d8a6e;
}

.posted-time i {
	margin-right: 5px;
}

.article-content p {
	font-size: 15px;
}

article.post {
	padding: 0 0 30px;
	margin: 0 0 30px;
	border-bottom: 1px solid #eee;
}

article.post:last-child {
	padding: 0;
	margin: 0;
	border-bottom: none;
}

article.post .thumbnail-img img {
	transition: all 0.6s ease;
	-webkit-transition: all 0.6s ease;
	-moz-transition: all 0.6s ease;
}

article.post:hover .thumbnail-img img {
	transform: scale(1.2,1.2);
}

a.readmore {
	display: block;
	margin-top: 15px;
	font-size: 15px;
	color: #675e5e;
}

a.readmore i {
    margin-left: 3px;
    transition: all 0.3s ease-in-out;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
}

a.readmore:hover i {
	margin-left: 8px;
}

article.post:hover .article-content h5 {
	color: #75c01f;
}

.site-pagination {
	margin-top: 30px;
	text-align: center;
}

.site-pagination .pagination {
	display: block;
}

.site-pagination .pagination > li {
    display: inline-block;
    vertical-align: top;
    margin-right: 7px;
}

.site-pagination .pagination > li:first-child > a, 
.site-pagination .pagination > li:first-child > span, 
.site-pagination .pagination > li:last-child > a, 
.site-pagination .pagination > li:last-child > span {
	border-radius: 0px;
}

.site-pagination .pagination > .active > a, 
.site-pagination .pagination > .active > span, 
.site-pagination .pagination > .active > a:hover, 
.site-pagination .pagination > .active > span:hover, 
.site-pagination .pagination > .active > a:focus, 
.site-pagination .pagination > .active > span:focus {
    background: #75c01f;
    border-color: #75c01f;
    color: #fff;
}

.site-pagination .pagination > li > a, 
.site-pagination .pagination > li > span {
    color: #000;
    padding: 6px 12px;
    border: 1px solid #ddd;
    transition: all 0.3s ease-in-out;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    font-size: 15px;
    font-weight: bold;
}

.site-pagination .pagination > li > a:hover {
    background: #75c01f;
    border-color: #75c01f;
    color: #fff;
}


/*=======================
404 ERROR PAGE
=======================*/

.pagecontent.errorpage {
	padding: 60px 0;
}

.error-inner {
	padding: 80px 0;
	background: #f3f3f3;
	text-align: center;
}

.error-inner h4 {
    font-size: 50px;
    color: red;
    margin: 0 0 10px;
}

.error-inner p {
	font-size: 25px;
	margin: 0 0 30px;
}

.error-inner a {
	display: inline-block;
	border: 2px solid #75c01f;
	color: #000;
	padding: 8px 25px 8px 60px;
	border-radius: 30px;
